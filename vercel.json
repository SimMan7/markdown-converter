{
  "version": 2,
  "functions": {
    "api/download/[...params].js": {
      "maxDuration": 30
    },
    "api/upload.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/blog",
      "dest": "/blog.html"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/blog/$1.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/ads.txt",
      "dest": "/ads.txt"
    }
  ],
  "public": true
} 